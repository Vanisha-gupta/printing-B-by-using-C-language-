#include<stdio.h>
int main()
{
  int c,r,row;
printf("enter row number of row should be greater or equal to 4:-");
scanf("%d",&row);
for(r=0;r<=row;r++)
{ 
  for(c=0;c<=row/2;c++)
  {
    if((c==0||c==row/2)||r==0||r==row/2||r==row)
      printf("*");
    else
    printf(" "); 
  } 
printf("\n");
}
}
   
